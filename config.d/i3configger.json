{
  "bars": {
    "defaults": {
      "font": "pango:Deja Vu Sans Mono, Font Awesome 5 Free, Font Awesome 5 Free Solid, Font Awesome 5 Brands 7",
      "mode": "dock",
      "position": "bottom",
      "select": "clock",
      "separator_symbol": " \u2551 ",
      "target": "..",
      "template": "tpl",
      "tray_padding": "0"
    },
    "targets": {
      "laptop": {
        "output": "eDP-1",
        "position": "bottom",
        "select": "full"
      },
      "left-external": {
        "output": "DP-2-2-8",
        "position": "bottom",
        "select": "full"
      },
      "right-external": {
        "output": "DP-1-2",
        "position": "bottom",
        "select": "full"
      },
      "vga": {
        "font": "pango:Deja Vu Sans Mono, Font Awesome 5 Free, Font Awesome 5 Free Solid, Font Awesome 5 Brands 10",
        "mode": "dock",
        "output": "VGA-0",
        "position": "bottom"
      }
    }
  },
  "main": {
    "i3_refresh_msg": "restart",
    "status_command": "py3status",
    "log": null,
    "notify": false,
    "target": "../config"
  }
}
