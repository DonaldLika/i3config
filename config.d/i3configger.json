{
  "bars": {
    "defaults": {
      "font": "pango:Fira Code 14, Font Awesome 5 Free 10, Font Awesome 5 Free Solid 10, Font Awesome 5 Brands 10",
      "mode": "dock",
      "position": "bottom",
      "select": "clock",
      "separator_symbol": " \u2551 ",
      "target": "..",
      "template": "tpl",
      "tray_padding": "1"
    },
    "targets": {
      "laptop-internal": {
        "output": "eDP-1",
        "position": "bottom",
        "select": "full"
      },
      "avira-left": {
        "output": "DP-2-2-8",
        "position": "bottom",
        "select": "full"
      },
      "avira-right": {
        "output": "DP-1-2",
        "position": "bottom",
        "select": "full"
      },
      "home-left": {
        "mode": "hide",
        "output": "DP-1-1",
        "position": "bottom",
        "select": "full"
      },
      "home-right": {
        "mode": "hide",
        "output": "DP-1-2-8",
        "position": "bottom",
        "select": "full"
      },
      "HDMI": {
        "mode": "dock",
        "output": "HDMI-1",
        "position": "bottom",
        "select": "full"
      }
    }
  },
  "main": {
    "i3_refresh_msg": "restart",
    "log": null,
    "notify": true,
    "target": "../config"
  }
}
